{
  "name": "random-names",
  "version": "1.0.0",
  "description": "Random names generator",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "watch:test": "npm t -- --watch",
    "test": "nyc mocha src/index.test.js",
    "lint": "eslint src",
    "validate": "npm-run-all --parallel test lint build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsflor/random-names.git"
  },
  "keywords": [
    "random",
    "names",
    "generator"
  ],
  "files": [
    "dist"
  ],
  "author": "Juan Sebastian Flor <sebastian-flor@hotmail.com> (https://github.com/jsflor)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsflor/random-names/issues"
  },
  "homepage": "https://github.com/jsflor/random-names#readme",
  "dependencies": {
    "unique-random-array": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "eslint": "3.2.0",
    "eslint-config-kentcdodds": "^9.0.0",
    "ghooks": "^2.0.4",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  }
}
