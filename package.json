{
  "name": "random-names",
  "version": "1.0.0",
  "description": "Random names generator",
  "main": "src/index.js",
  "scripts": {
    "watch:test": "npm t -- --watch",
    "test": "nyc mocha src/index.test.js",
    "lint": "eslint src",
    "validate": "npm-run-all --parallel test lint"
  },
  "keywords": [
    "random",
    "names",
    "generator"
  ],
  "author": "Juan Sebastian Flor <sebastian-flor@hotmail.com> (https://github.com/jsflor)",
  "license": "MIT",
  "dependencies": {
    "unique-random-array": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "3.2.0",
    "eslint-config-kentcdodds": "^9.0.0",
    "ghooks": "^2.0.4",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  }
}
